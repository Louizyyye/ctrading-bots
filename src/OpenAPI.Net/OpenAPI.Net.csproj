<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/afhacker/OpenAPI.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/afhacker/OpenAPI.Net</RepositoryUrl>
    <PackageTags>cTrader, Open API, Spotware</PackageTags>
    <Description>A .NET RX library for Spotware Open API</Description>
    <PackageId>Spotware.OpenAPI.Net</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.15.2" />
    <PackageReference Include="RestSharp" Version="106.11.7" />
    <PackageReference Include="System.Reactive" Version="5.0.0" />
    <PackageReference Include="System.Text.Json" Version="5.0.1" />
  </ItemGroup>

</Project>
